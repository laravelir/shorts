{
    "name": "laravelir/shorts",
    "description": "a simple laravel url shortener",
    "homepage": "https://github.com/laravelir/shorts",
    "type": "library",
    "version": "0.1.0",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel url shortener",
        "laravel shorts"
    ],
    "authors": [
        {
            "name": "laravelir",
            "email": "laravelir@outlook.com",
            "role": "maintainer",
            "homepage": "https://github.com/laravelir"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravelir\\Shorts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelir\\Shorts\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shorts\\Shorts\\Providers\\ShortsServiceProvider"
            ],
            "aliases": {
                "Shorts": "Laravelir\\Shorts\\Facades\\Shorts"
            }
        }
    },
    "require": {
        "php": ">=7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
